    "@types/mocha": "^10.0.10",
    "@types/photoshop": "^25.0.4",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.18.1",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
